{
  "name": "twitter-tdd",
  "private": true,
  "version": "1.0.0",
  "description": "Twitter Test-Drive Application",
  "main": "dist/index.js",
  "scripts": {
    "test": "node_modules/.bin/nyc node_modules/.bin/ava",
    "coverage": "npm test && node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls",
    "compile": "node_modules/.bin/babel src -d dist",
    "lint": "find src -name \"*.js\" | xargs node_modules/.bin/eslint",
    "prepublish": "npm run compile",
    "start": "node dist/index.js"
  },
  "author": "Victor Gama <hey@vito.io>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.6.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-es2015-classes": "^6.6.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.6.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.2.0",
    "nyc": "^5.6.0",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "cheerio": "^0.20.0",
    "consolidate": "^0.14.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "json2csv": "^3.2.0",
    "mongoose": "^4.4.7",
    "node-twitter-api": "^1.7.0",
    "raven": "^0.10.0",
    "slug": "^0.9.1",
    "swig": "^1.4.2",
    "unfurl-url": "^1.0.0"
  },
  "ava": {
    "files": [
      "spec/*.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ]
  }
}
